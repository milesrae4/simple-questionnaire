{
  "name": "questionnaire-web-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.20.2"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "axios": "^0.27.2",
    "dotenv": "^16.0.0",
    "questionnaire-shared": "*",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dom-confetti": "^0.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "dev": "react-scripts start",
    "bundle": "react-scripts build",
    "build": "tsc -b",
    "build:watch": "yarn build --watch",
    "build:clean": "yarn clean && yarn build",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "lint": "eslint --ext .ts,.tsx src",
    "test": "react-scripts test --verbose --watchAll=false",
    "test:watch": "react-scripts test --verbose",
    "test:coverage": "react-scripts test --verbose --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "11",
    "@testing-library/user-event": "^12.1.10",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.12",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.0"
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  }
}
